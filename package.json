{
  "name": "commitlint-config-travi",
  "version": "0.0.0-semantically-released",
  "description": "shareable config for commitlint",
  "license": "MIT",
  "author": "Matt Travi",
  "repository": "travi/commitlint-config-travi",
  "bugs": "https://github.com/travi/commitlint-config-travi/issues",
  "homepage": "https://github.com/travi/commitlint-config-travi#readme",
  "main": "index.js",
  "scripts": {
    "lint:md": "globstar --node -- markdownlint **/*.md",
    "lint:js": "eslint . --cache",
    "test": "run-s lint:*",
    "commitmsg": "commitlint -e",
    "precommit": "npm test",
    "prepack": "npm test"
  },
  "publishConfig": {
    "access": "public"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@travi/eslint-config-travi": "1.6.18",
    "globstar": "1.0.0",
    "husky": "1.1.2",
    "markdownlint-cli": "0.13.0",
    "npm-run-all": "4.1.3"
  },
  "dependencies": {
    "@commitlint/cli": "7.2.1",
    "@commitlint/config-conventional": "7.1.2",
    "cz-conventional-changelog": "2.1.0"
  }
}
