{
  "name": "commitlint-config-travi",
  "version": "0.0.0-semantically-released",
  "description": "shareable config for commitlint",
  "license": "MIT",
  "author": "Matt Travi",
  "repository": "travi/commitlint-config-travi",
  "bugs": "https://github.com/travi/commitlint-config-travi/issues",
  "homepage": "https://github.com/travi/commitlint-config-travi#readme",
  "main": "index.js",
  "scripts": {
    "lint:md": "globstar --node -- markdownlint **/*.md",
    "lint:js": "eslint . --cache",
    "test": "run-s lint:*",
    "commitmsg": "commitlint -e",
    "precommit": "npm test",
    "preversion": "npm test",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "publishConfig": {
    "access": "public"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/@commitlint/prompt"
    }
  },
  "devDependencies": {
    "@commitlint/prompt": "4.1.0",
    "@travi/eslint-config-travi": "1.5.6",
    "globstar": "1.0.0",
    "husky": "0.14.3",
    "markdownlint-cli": "0.3.1",
    "npm-run-all": "4.1.1",
    "semantic-release": "8.0.3"
  },
  "dependencies": {
    "@commitlint/cli": "4.0.0",
    "@commitlint/config-angular": "3.1.1"
  }
}
