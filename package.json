{
  "name": "commitlint-config-travi",
  "version": "0.0.0-semantically-released",
  "description": "shareable config for commitlint",
  "license": "MIT",
  "author": "Matt Travi",
  "repository": "travi/commitlint-config-travi",
  "bugs": "https://github.com/travi/commitlint-config-travi/issues",
  "homepage": "https://github.com/travi/commitlint-config-travi#readme",
  "main": "index.js",
  "scripts": {
    "lint:md": "globstar --node -- markdownlint **/*.md",
    "lint:js": "eslint . --cache",
    "test": "run-s lint:*",
    "commitmsg": "commitlint -e",
    "precommit": "npm test",
    "preversion": "npm test",
    "semantic-release": "semantic-release",
    "greenkeeper:update-lockfile": "greenkeeper-lockfile-update",
    "greenkeeper:upload-lockfile": "greenkeeper-lockfile-upload"
  },
  "publishConfig": {
    "access": "public"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@travi/eslint-config-travi": "1.6.6",
    "globstar": "1.0.0",
    "greenkeeper-lockfile": "1.13.2",
    "husky": "0.15.0-rc.2",
    "markdownlint-cli": "0.6.0",
    "npm-run-all": "4.1.2",
    "semantic-release": "12.4.1"
  },
  "dependencies": {
    "@commitlint/cli": "6.0.2",
    "@commitlint/config-conventional": "6.0.2",
    "cz-conventional-changelog": "2.1.0"
  }
}
