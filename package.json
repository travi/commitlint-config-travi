{
  "name": "commitlint-config-travi",
  "description": "shareable config for commitlint",
  "license": "MIT",
  "version": "0.0.0-semantically-released",
  "author": "Matt Travi",
  "repository": "travi/commitlint-config-travi",
  "bugs": "https://github.com/travi/commitlint-config-travi/issues",
  "homepage": "https://github.com/travi/commitlint-config-travi#readme",
  "main": "index.js",
  "scripts": {
    "lint:md": "remark --quiet --frail .",
    "lint:js": "eslint . --cache",
    "lint:peer": "npm ls >/dev/null",
    "test": "npm-run-all --print-label --parallel lint:*",
    "prepack": "npm test",
    "prepare": "husky install",
    "lint:publish": "publint --strict"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@form8ion/commitlint-config": "1.0.55"
  },
  "devDependencies": {
    "@travi/eslint-config-travi": "1.8.8",
    "cz-conventional-changelog": "3.3.0",
    "husky": "8.0.3",
    "npm-run-all2": "5.0.0",
    "publint": "0.2.2",
    "remark-cli": "11.0.0",
    "remark-preset-lint-travi": "6.0.1"
  }
}
