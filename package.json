{
  "name": "commitlint-config-travi",
  "version": "0.0.0-semantically-released",
  "description": "shareable config for commitlint",
  "license": "MIT",
  "author": "Matt Travi",
  "repository": "travi/commitlint-config-travi",
  "bugs": "https://github.com/travi/commitlint-config-travi/issues",
  "homepage": "https://github.com/travi/commitlint-config-travi#readme",
  "main": "index.js",
  "scripts": {
    "lint:md": "remark --quiet --frail .",
    "lint:js": "eslint . --cache",
    "test": "npm-run-all --print-label --parallel lint:*",
    "commitmsg": "commitlint -e",
    "precommit": "npm test",
    "prepack": "npm test"
  },
  "publishConfig": {
    "access": "public"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@travi/eslint-config-travi": "1.8.0",
    "husky": "2.2.0",
    "npm-run-all": "4.1.5",
    "remark-cli": "6.0.1",
    "remark-preset-lint-travi": "1.2.0"
  },
  "dependencies": {
    "@commitlint/cli": "7.6.1",
    "@commitlint/config-conventional": "7.6.0",
    "cz-conventional-changelog": "2.1.0"
  }
}
